{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "allowJs": false,
    "inlineSourceMap": true,
    "outDir": "./build",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react"
